{
  "rewrites": [
    {
      "source": "/api/stuff/:match*",
      "destination": "/api/stuff/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    },
    {
      "source": "/anvil/:match*",
      "destination": "https://testapp8dev.anvil.app/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}