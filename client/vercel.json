{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    },
    {
      "source": "/anvil/:match*",
      "destination": "https://testapp8dev.anvil.app/:match*",
      "headers": [{ "key": "X-Routed-Through", "value": "Vercel-Anvil-Proxy" }]
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
